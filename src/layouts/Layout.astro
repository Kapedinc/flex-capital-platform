                  <script>
        let t1 = gsap.timeline({ paused: true, reversed: true });
let watch = false;
const card = document.querySelector(".container");
card.addEventListener("click", (e) => {
  if (!watch) {
    t1.play();
    watch = true;
  } else {
    t1.reverse();
    watch = false;
  }
});
t1.to(
  ".container",
  {
    ease: "power1.inOut",
    height: 280,
  },
  0
);
t1.to(
  ".card-balance",
  {
    ease: "power1.inOut",
    y: 80,
    x: 20,
    scale: 1.1,
  },
  0.5
);
t1.to(
  ".card-name",
  {
    ease: "power1.inOut",
    y: 80,
  },
  0.5
);
t1.to(
  ".logo",
